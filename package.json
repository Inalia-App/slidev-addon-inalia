{
  "name": "slidev-addon-inalia",
  "type": "module",
  "version": "0.1.19",
  "packageManager": "pnpm@10.8.1",
  "keywords": [
    "slidev",
    "slidev-addon",
    "inalia"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./components/*": {
      "import": "./dist/components/*"
    },
    "./composables/*": {
      "types": "./dist/composables/*.d.ts",
      "import": "./dist/composables/*.mjs"
    },
    "./utils/*": {
      "types": "./dist/utils/*.d.ts",
      "import": "./dist/utils/*.mjs"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./index.d.ts",
  "files": [
    "components",
    "composables",
    "dist",
    "global-bottom.vue",
    "index.d.ts",
    "layouts",
    "pages",
    "setup",
    "slidev.d.ts",
    "styles.css",
    "tsconfig.json",
    "types",
    "utils",
    "vite-env.d.ts",
    "vite.config.ts",
    "window.d.ts"
  ],
  "engines": {
    "slidev": ">=0.49.29"
  },
  "scripts": {
    "build": "slidev build",
    "prepack": "unbuild",
    "dev": "slidev --open",
    "export": "slidev export",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "npm run lint && changelogen --release && npm publish && git push --follow-tags"
  },
  "dependencies": {
    "@slidev/cli": "^51.5.0",
    "@slidev/theme-default": "latest",
    "@unovis/ts": "1.4.3-beta.0",
    "@unovis/vue": "1.4.3-beta.0",
    "laravel-echo": "^1.19.0",
    "ofetch": "^1.4.1",
    "pusher-js": "8.4.0-rc2",
    "rc9": "^2.1.2",
    "reka-ui": "^2.3.2",
    "uncrypto": "^0.1.3",
    "uqr": "^0.1.2",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.12.0",
    "@unocss/eslint-plugin": "^0.64.1",
    "changelogen": "^0.5.7",
    "eslint": "^9.24.0",
    "mkdist": "^1.6.0",
    "typescript": "^5.8.3",
    "unbuild": "^2.0.0"
  }
}
